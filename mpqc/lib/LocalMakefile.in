# Emacs should use -*- Makefile -*- mode.

host = @host@
host_cpu = @host_cpu@
host_vendor = @host_vendor@
host_os = @host_os@

target = @target@
target_cpu = @target_cpu@
target_vendor = @target_vendor@
target_os = @target_os@

# The object code suffix
OBJSUF = @OBJSUF@

# The library suffix
LIBSUF = @LIBSUF@

CC  = @CC@
CXX = @CXX@

AR = @AR@
ARFLAGS = r

LD = $(CXX)
LDFLAGS =

INSTALL = @INSTALL@
INSTALLLIBOPT = -m 0644
INSTALLBINOPT = -m 0755 -s

CTANGLE = @CTANGLE@
CWEAVE = @CWEAVE@
FLEX = @FLEX@
BISON = @BISON@
RANLIB = @RANLIB@
PERL = @PERL@
GENCLASS = @GENCLASS@
TMPL2MACDEP = $(SRCDIR)/$(TOPDIR)/bin/tmpl2mac.pl
TMPL2MAC = $(PERL) $(TMPL2MACDEP)
COMPRESS = gzip
UNCOMPRESS = gzip -d
SGEN=@SGEN@
LISTLIBS=@LISTLIBS@ -DLIBSUF=$(LIBSUF)
TMPL=@TMPL@

DEFINES = @DEFS@

INCLUDE += @EXTRAINCLUDE@
CPPFLAGS = $(DEFINES) $(INCLUDE)

COPTIONS_OPT = @COPTIONS_OPT@
COPTIONS_DBG = @COPTIONS_DBG@
COPTIONS_MISC = @COPTIONS_MISC@

CFLAGS_OPT = $(COPTIONS_OPT) $(COPTIONS_MISC)
CFLAGS_DBG = $(COPTIONS_DBG) $(COPTIONS_MISC)
CFLAGS_OPTDBG = $(COPTIONS_OPT) $(COPTIONS_DBG) $(COPTIONS_MISC)
CFLAGS = @CFLAGS@

CXXOPTIONS_OPT = @CXXOPTIONS_OPT@
CXXOPTIONS_DBG = @CXXOPTIONS_DBG@
CXXOPTIONS_MISC = @CXXOPTIONS_MISC@

CXXFLAGS_OPT = $(CXXOPTIONS_OPT) $(CXXOPTIONS_MISC)
CXXFLAGS_DBG = $(CXXOPTIONS_DBG) $(CXXOPTIONS_MISC)
CXXFLAGS_OPTDBG = $(CXXOPTIONS_OPT) $(CXXOPTIONS_DBG) $(CXXOPTIONS_MISC)
CXXFLAGS = @CXXFLAGS@

HIGHOPT_CFLAGS = $(CFLAGS)
NOALIAS_CFLAGS =

HIGHOPT_CXXFLAGS = $(CXXFLAGS)
NOALIAS_CXXFLAGS = $(NOALIAS_CFLAGS)

ELF = @ELF@
ELF_MAJOR = @ELF_MAJOR@
ELF_MINOR = @ELF_MINOR@
SH_LD = @SH_LD@
SH_LDFLAGS = @SH_LDFLAGS@

HAVE_SYSV_IPC = @HAVE_SYSV_IPC@
HAVE_HRECV = @HAVE_HRECV@
HAVE_NX = @HAVE_NX@
HAVE_PVM = @HAVE_PVM@
HAVE_MPI = @HAVE_MPI@
HAVE_MPL = @HAVE_MPL@

SYSLIBS = @LIBS@
