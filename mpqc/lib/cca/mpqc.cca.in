<?xml version="1.0"?>
<libInfo>

<scl>
  <library uri="@sclibdir@/cca/mpqc.so"
   scope="global" resolution="now" >
    <class name="MPQC.MoleculeViewer" desc="ior/impl" />
    <class name="MPQC.Units" desc="ior/impl" />
    <class name="MPQC.ModelFactory" desc="ior/impl" />
    <class name="MPQC.GaussianBasisShell" desc="ior/impl" />
    <class name="MPQC.GaussianBasisAtomic" desc="ior/impl" />
    <class name="MPQC.GaussianBasisMolecular" desc="ior/impl" />
    <class name="MPQC.Model" desc="ior/impl" />
    <class name="MPQC.CoordinateModel" desc="ior/impl" />
    <class name="MPQC.OptimizationSolver" desc="ior/impl" />
    <class name="MPQC.IntV3EvaluatorFactory" desc="ior/impl" />
    <class name="MPQC.CintsEvaluatorFactory" desc="ior/impl" />
    <class name="MPQC.Libint2EvaluatorFactory" desc="ior/impl" />
    <class name="MPQC.SimpleDriver" desc="ior/impl" />
  </library>
</scl>

<componentDeployment 
  name="MPQC.MoleculeViewer"
  paletteClassAlias="MPQC.MoleculeViewer" >
  <environment> 
    <ccaSpec binding="babel" />
    <library loading="dynamic"
             name="@prefix@/lib/cca/mpqc.so"
             libtool-archive="@prefix@/lib/cca/mpqc.la"
             shared-archive="@prefix@/lib/cca/mpqc.so" />
    <headers path="@prefix@/include/chemistry/cca"
             files="MPQC_MoleculeViewer" />
  </environment>
</componentDeployment>

