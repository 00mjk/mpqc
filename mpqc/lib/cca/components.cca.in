<?xml version="1.0"?>
<libInfo>

<scl>
  <library uri="@sclibdir@/cca/mpqc.so"
   scope="global" resolution="now" >
    <class name="MPQC.Chemistry_MoleculeViewer" desc="ior/impl" />
    <class name="MPQC.Physics_Units" desc="ior/impl" />
    <class name="MPQC.Chemistry_QC_ModelFactory" desc="ior/impl" />
    <class name="MPQC.Chemistry_QC_GaussianShell" desc="ior/impl" />
    <class name="MPQC.Chemistry_QC_GaussianBasisSet" desc="ior/impl" />
    <class name="MPQC.Chemistry_QC_Model" desc="ior/impl" />
    <class name="MPQC.ChemistryOpt_CoordinateModel" desc="ior/impl" />
    <class name="MPQC.IntegralEvaluatorFactory" desc="ior/impl" />
    <class name="MPQC.SimpleDriver" desc="ior/impl" />
  </library>
</scl>

<componentDeployment 
  name="MPQC.Chemistry_MoleculeViewer"
  paletteClassAlias="MPQC.Chemistry_MoleculeViewer" >
  <environment> 
    <ccaSpec binding="babel" />
    <library loading="dynamic"
             name="@prefix@/lib/cca/mpqc.so"
             libtool-archive="@prefix@/lib/cca/mpqc.la"
             shared-archive="@prefix@/lib/cca/mpqc.so" />
    <headers path="@prefix@/include/chemistry/cca"
             files="MPQC_Chemistry_MoleculeViewer" />
  </environment>
</componentDeployment>

