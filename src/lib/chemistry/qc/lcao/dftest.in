test: (
   test_processor = 1
   basis = $:basis
   fitbasis = $:fitbasis
   storage = 10000000
   store_integrals = no
   print_centers = no
   overlap = no
   kinetic = no
   hcore = no
   nuclear = no
   3 = no
   bounds = no
   4 = no
   timestats = no
   4der = no
   print = no
   printbounds = no
   niter = 1
   boundstats = yes
   permute = no
   unique = yes
   %quartet = [0 0 0 1]
   %storage = 10000
   wfn = $:wfn
)   

stdbasis<SplitBasisSet>: (
  basis = $:stdmbasis
)
stdmbasis<GaussianBasisSet>: (
  %basisdir = "./"
  %basisfiles = [ "tmp" ]
  molecule = $:molecule
  name = "DZ (Dunning)"
  %name = "1s"
  %name = "STO-3G"
  %puream = true
)

stdfitbasis<UncontractedBasisSet>: (
  basis = $:stdfitmbasis
)

stdfitmbasis<GaussianBasisSet>: (
  molecule = $:molecule
  name = "aug-cc-pVDZ"
)
