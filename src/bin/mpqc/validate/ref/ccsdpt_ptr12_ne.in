integral<IntegralLibint2>:()

mol<Molecule>: (
  symmetry = auto
  unit = bohr
  { atoms geometry } = {
    Ne [0.0 0.0 0.0]
  }
)
molecule = $:mol

basis<GaussianBasisSet>: (
  name = cc-pVDZ
  puream = true
  molecule = $:molecule
)

abasis<GaussianBasisSet>: (
  name = cc-pVTZ
  puream = true
  molecule = $:molecule
)

mole<CCSD>: (
  perturbative = "(T)R12"

  integrals = $:integral
  molecule = $:molecule
  basis = $:basis
  aux_basis = $:abasis
  corr_factor = stg-3g
  corr_param = 1.5
  stdapprox = "C"
  ebc = false
  gbc = false
  ccthresh  = 1.0e-10
  abs_method = cabs+
  memory     = 200000000
  workmemory = 100000000
  store_ints = posix
  nfzc = auto
  ansatz<R12Ansatz> : ( diag = true )
  reference<CLHF>: (
    integrals = $:integral
    molecule = $:molecule
    basis = $:basis
    memory = 2400000
  )
)

mpqc: (
  mole = $:mole
  savestate = false
  checkpoint = false
)

