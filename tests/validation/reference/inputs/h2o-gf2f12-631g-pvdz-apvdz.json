{
  "units" : "2010CODATA",
  "property":{
    "type" : "GFRealPole",
    "precision" : 1.0e-6,
    "target" : -1,
    "wfn" : "$:wfn"
  },
  "atoms": {
    "file_name": "n2.xyz",
    "sort_input": true,
    "charge": 0,
    "n_cluster" : 2
  },
  "basis": {
    "name": "aug-cc-pVDZ",
    "atoms": "$:atoms"
  },
  "df_basis": {
    "name": "aug-cc-pVQZ-RI",
    "atoms": "$:atoms"
  },
  "aux_basis":{
    "name": "aug-cc-pVDZ-CABS",
    "atoms": "$:atoms"
  },
  "wfn_world":{
    "atoms" : "$:atoms",
    "basis" : "$:basis",
    "df_basis" :"$:df_basis",
    "aux_basis" : "$:aux_basis",
    "f12_factor" : "stg-6g[1.1]"
  },
  "scf":{
    "type": "RI-RHF",
    "atoms": "$:atoms",
    "wfn_world": "$:wfn_world"
  },
  "wfn":{
    "max_iter": 25,
    "type": "GF2F12",
    "atoms": "$:atoms",
    "wfn_world": "$:wfn_world",
    "ref": "$:scf",
    "frozen_core" : false,
    "use_cabs" : true,
    "self_energy" : "gf2H",
    "dyson_method" : "nondiagonal",
    "f12_method" : "diagonal"
  }
}

