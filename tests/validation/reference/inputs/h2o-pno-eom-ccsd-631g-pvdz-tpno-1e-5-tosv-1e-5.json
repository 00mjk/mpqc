{
  "units": "2010CODATA",
  "molecule": {
    "type": "Molecule",
    "file_name": "h2o.xyz",
    "attach_hydrogen" : false,
    "charge": 0,
    "n_cluster": 2
  },
  "obs": {
    "name": "6-31G",
    "molecule": "$:molecule"
  },
  "df_bs" : {
    "name": "cc-pVDZ",
    "molecule": "$:molecule"
  },
  "wfn_world":{
    "type" : "WfnWorld",
    "molecule" : "$:molecule",
    "basis" : "$:obs",
    "df_basis" : "$:df_bs"
  },
  "scf":{
    "type": "RI-RHF",
    "wfn_world": "$:wfn_world",
    "localizer": { "type": "FosterBoysLocalizer" },
    "localize_core" : false
  },
  "wfn":{
    "type": "EOM-CCSD",
    "wfn_world": "$:wfn_world",
    "molecule" : "$:molecule",
    "ref": "$:scf",
    "max_iter": 50,
    "method" : "df",
    "occ_block_size" : 4,
    "unocc_block_size" : 4,
    "solver" : "pno",
    "pno_canonical" : true,
    "tpno" : 1.0e-5,
    "tosv" : 1.0e-5,
    "eom_pno" : "state-average",
    "eom_tpno" : 1.0e-5,
    "eom_tosv" : 1.0e-5,
    "tiling_method" : "flexible"
  },
  "property" : {
    "type" : "ExcitationEnergy",
    "precision" : "1e-7",
    "n_roots" : 4,
    "wfn" : "$:wfn"
  }
}
