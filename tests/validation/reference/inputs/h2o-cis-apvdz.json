  "property": {
    "type" : "ExcitationEnergy",
    "wfn" : "$:wfn",
    "n_roots" : 4,
    "triplets" : true
  },
  "molecule": {
    "file_name": "h2o.xyz",
    "attach_hydrogen": false,
    "n_cluster": 2
  },
  "basis": {
    "name": "aug-cc-pVDZ",
    "molecule": "$:molecule"
  },
  "wfn_world":{
    "type" : "WfnWorld",
    "molecule" : "$:molecule",
